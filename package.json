    "eslint": "5.16.0",
    "eslint-plugin-flowtype": "3.8.2",
    "flow-bin": "0.98.1",
    "jest": "24.8.0",
    "prettier": "1.17.1",
    "rimraf": "2.6.3",
    "rollup": "1.11.3",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.3.4",
    "rollup-plugin-ignore": "1.0.5",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
